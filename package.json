{
  "name": "@kohui/react",
  "version": "0.0.0-alpha.2",
  "license": "MIT",
  "author": {
    "name": "kohai",
    "email": "i@kohai.dev",
    "url": "https://kohai.dev"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --name kohui-react",
    "build": "tsdx build --name kohui-react",
    "test": "tsdx test --passWithNoTests --name kohui-react",
    "lint": "tsdx lint --name kohui-react",
    "prepare": "tsdx build --name kohui-react",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/kohui-react.esm.js",
  "size-limit": [
    {
      "path": "dist/kohui-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/kohui-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addons": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.22",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "styled-components": "^5.3.3"
  }
}